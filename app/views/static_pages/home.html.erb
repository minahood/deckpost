<% if logged_in? %>
  <div class="row">
    
    <aside class="col-md-4 col-lg-3 right-border-md">
      <%= render 'shared/user_info' %>
    </aside>
    
    <div class="col-md-8 col-lg-6">
      <%= render 'shared/user_nav' %>
      
      <%= render 'shared/feed' %>
    </div>
  </div>
  

  
<% else %>
  
  <%= render 'shared/search_form' %>
<% end %>

<script type="text/javascript">
  $("#user_nav-link-home").addClass("active");
</script>
    